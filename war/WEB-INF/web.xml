<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
    <servlet>
     <servlet-name>index2-jsp</servlet-name>
     <jsp-file>/WEB-INF/index2.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
     <servlet-name>index2-jsp</servlet-name>
     <url-pattern>/index2.jsp</url-pattern>
    </servlet-mapping>
    <servlet>
     <servlet-name>class_home-jsp</servlet-name>
     <jsp-file>class_home.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
     <servlet-name>class_home-jsp</servlet-name>
     <url-pattern>/class_home.jsp</url-pattern>
    </servlet-mapping>
    <servlet>
     <servlet-name>topics-jsp</servlet-name>
     <jsp-file>/WEB-INF/topics.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
     <servlet-name>topics-jsp</servlet-name>
     <url-pattern>/topics.jsp</url-pattern>
    </servlet-mapping>
	<servlet>
		<servlet-name>Forum_v1</servlet-name>
		<servlet-class>forum_v1.Forum_v1Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Forum_v1</servlet-name>
		<url-pattern>/forum_v1</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>forum_v1.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>tag</servlet-name>
		<servlet-class>forum_v1.tagServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tag</servlet-name>
		<url-pattern>/tag</url-pattern>
	</servlet-mapping>
    
    <servlet>
		<servlet-name>Adv_query</servlet-name>
		<servlet-class>forum_v1.Adv_queryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Adv_query</servlet-name>
		<url-pattern>/adv_query</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value>spi.EndPoints</param-value>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>